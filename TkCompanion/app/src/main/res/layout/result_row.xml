<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/session"
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:text="2014-10-23"/>

    <TextView
        android:id="@+id/competition"
        android:layout_width="90dip"
        android:layout_height="wrap_content"
        android:text="100F"
        android:layout_below="@+id/swim_name"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cosima"
        android:id="@+id/swim_name"
        android:textSize="20sp"
        android:layout_toEndOf="@+id/session"
        android:layout_marginLeft="55dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total: "
        android:id="@+id/textView5"
        android:layout_alignStart="@+id/swim_name"
        android:layout_below="@+id/swim_name"
        android:textSize="15sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00:00.0"
        android:id="@+id/total_time"
        android:layout_alignParentBottom="false"
        android:layout_toEndOf="@+id/textView5"
        android:textSize="15sp"
        android:layout_below="@+id/swim_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avg: "
        android:id="@+id/textview1"
        android:layout_toEndOf="@+id/total_time"
        android:textSize="15sp"
        android:layout_alignBottom="@+id/total_time"
        android:layout_marginLeft="@dimen/activity_vertical_margin" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00:00.0"
        android:id="@+id/avg_data"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/textview1"
        android:layout_toEndOf="@id/textview1"
        android:textSize="15sp" />

</RelativeLayout>
